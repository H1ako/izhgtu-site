{
  "name": "frontend-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "apollo:schema": "apollo service:download --endpoint=http://127.0.0.1:8000/api/graphql/ ./graphql/graphql-schema.json",
    "apollo:generate": "npx apollo codegen:generate --localSchemaFile=./graphql/graphql-schema.json --target typescript --tagName=gql --excludes=node_modules/**/* --includes=./graphql/queries/*.tsx --endpoint=http://127.0.0.1:8000/api/graphql/ --outputFlat ./graphql/generated.tsx",
    "apollo:update_schema": "npm run apollo:schema && npm run apollo:generate"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@graphql-codegen/cli": "^2.14.0",
    "@graphql-codegen/typescript": "^2.8.2",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-calendar": "^3.5.3",
    "@types/react-dom": "18.0.9",
    "@types/react-slick": "^0.23.10",
    "body-scroll-lock": "^4.0.0-beta.0",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "font-awesome": "^4.7.0",
    "graphql": "^14.7.0",
    "next": "13.0.3",
    "next-http-proxy-middleware": "^1.2.5",
    "react": "18.2.0",
    "react-calendar": "^3.9.0",
    "react-dom": "18.2.0",
    "react-fast-marquee": "^1.3.5",
    "react-intersection-observer": "^9.4.1",
    "react-slick": "^0.29.0",
    "recoil": "^0.7.6",
    "sass": "^1.55.0",
    "slick-carousel": "^1.8.1",
    "typescript": "4.9.3"
  }
}
